    "@antfu/eslint-config": "^0.38.4",
    "@changesets/cli": "^2.26.1",
    "@eslint/create-config": "^0.4.2",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "pnpm": "^8.1.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
}