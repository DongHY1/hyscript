{
  "compilerOptions": {
    "target": "es2018", 
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",                    
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "noImplicitAny": false, 
    "skipDefaultLibCheck": true,                     
    "skipLibCheck": true,
    "noEmit": true,
    "noUncheckedIndexedAccess": true                    
  }
}